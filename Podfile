# Uncomment the next line to define a global platform for your project
platform :osx, '10.13'

target 'Voltage' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Voltage
  pod 'BlueSocket'
  pod 'Fakery', '~> 3.0'
  pod 'SwiftLint'

  target 'VoltageTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Fakery', '~> 3.0'
  end

  target 'VoltageUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

